<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Envoi.
    -->
    <changeSet id="20200926055123-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="coli_id"
                                 baseTableName="app_envoi"
                                 constraintName="fk_app_envoi_coli_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_coli"/>

        <addForeignKeyConstraint baseColumnNames="expediteur_id"
                                 baseTableName="app_envoi"
                                 constraintName="fk_app_envoi_expediteur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_client"/>

        <addForeignKeyConstraint baseColumnNames="destinataire_id"
                                 baseTableName="app_envoi"
                                 constraintName="fk_app_envoi_destinataire_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_destinataire"/>

    </changeSet>
</databaseChangeLog>
