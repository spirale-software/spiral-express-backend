<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Destinataire.
    -->
    <changeSet id="20200926055323-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="personne_id"
                                 baseTableName="destinataire"
                                 constraintName="fk_destinataire_personne_id"
                                 referencedColumnNames="id"
                                 referencedTableName="personne"/>

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="destinataire"
                                 constraintName="fk_destinataire_client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

    </changeSet>
</databaseChangeLog>
